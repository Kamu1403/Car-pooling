<!--pages/personalinfo/personalinfo.wxml-->
<view class="weui-cells weui-cells_after-title">
  <navigator aria-role="" url="./changeheadimg" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view aria-role="link" aria-labelledby="js_cell_tl1_bd js_cell_tl1_ft" id="js_cell_tl1_bd" class="weui-cell__bd">头像</view>
    <view class="weui-cell__ft weui-cell__ft_in-access" id="js_cell_tl1_ft" aria-hidden="true">
      <image class="head_image" src="{{userheadimg}}"></image>
    </view>
  </navigator>
  <navigator aria-role="" url="./change?changeWhat=name" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">用户名</view>
    <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access" style="color: rgb(7, 6, 6);">{{username}}</view>
  </navigator>
  <navigator aria-role="" url="./changepwd" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">密码</view>
    <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">点击修改</view>
  </navigator>
</view>